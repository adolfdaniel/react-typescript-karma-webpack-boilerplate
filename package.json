    "build:dev": "webpack --env.dev",
    "start": "http-server ./dist",
    "dev": "webpack-dev-server --env.dev --hot",
    "lint-ts": "tslint \"src/**/*.tsx\" \"src/**/*.ts\" \"test/**/*.tsx\" \"test/**/*.ts\"",
    "test": "npx karma start karma.conf.js --singleRun true"
